{
  "name": "@tmcw/togeojson",
  "version": "1.0.0",
  "description": "convert KML and GPX to GeoJSON",
  "source": "index.js",
  "umd:main": "dist/togeojson.umd.js",
  "module": "dist/togeojson.es.js",
  "main": "dist/togeojson.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tape test/*.js",
    "prepublish": "microbundle",
    "pretest": "microbundle",
    "release": "standard-version",
    "cov": "istanbul cover test/index.js && coveralls < ./coverage/lcov.info"
  },
  "devDependencies": {
    "coveralls": "~2.11.12",
    "cz-conventional-changelog": "^1.2.0",
    "glob": "^7.0.5",
    "istanbul": "~0.4.4",
    "microbundle": "^0.9.0",
    "standard-version": "^4.4.0",
    "tape": "~4.6.0"
  },
  "bin": {
    "togeojson": "togeojson"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tmcw/togeojson.git"
  },
  "browser": {
    "xmldom": false
  },
  "license": "BSD-2-Clause",
  "dependencies": {
    "concat-stream": "~2.0.0",
    "minimist": "1.2.0",
    "xmldom": "~0.1.19"
  },
  "keywords": [
    "kml",
    "geojson",
    "gpx",
    "geo",
    "parser",
    "formatter",
    "input",
    "leaflet",
    "maps"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
